{
  "expo": {
    "name": "Silent Moon",
    "slug": "silent-moon-meditation",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#8E97FD"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "updates": {
      "enabled": false,
      "checkAutomatically": "ON_ERROR_RECOVERY",
      "fallbackToCacheTimeout": 0
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.silentmoon.meditation"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#8E97FD"
      },
      "package": "com.silentmoon.meditation",
      "versionCode": 1,
      "permissions": [
        "INTERNET",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "NOTIFICATIONS"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "build": {
          "experimental": {
            "ios": {
              "appExtensions": [
                {
                  "targetName": "OneSignalNotificationServiceExtension",
                  "bundleIdentifier": "com.silentmoon.meditation.OneSignalNotificationServiceExtension",
                  "entitlements": {
                    "com.apple.security.application-groups": [
                      "group.com.silentmoon.meditation.onesignal"
                    ]
                  }
                }
              ]
            }
          }
        },
        "projectId": "a6c9ca33-d59b-46c4-b72e-bffc28e85260"
      }
    },
    "plugins": [
      "@react-native-community/datetimepicker",
      "onesignal-expo-plugin",
      "expo-notifications"
    ]
  }
}
